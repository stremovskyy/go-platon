# Copy this file to examples/.env and replace values with real credentials.
PLATON_MERCHANT_NAME=demo-merchant
PLATON_MERCHANT_NAME_WITHDRAW=demo-merchant-withdraw
PLATON_LOGIN=demo-login
PLATON_MERCHANT_ID=1234
PLATON_MERCHANT_ID_WITHDRAW=5678
PLATON_MERCHANT_KEY=demo-merchant-key
PLATON_SECRET_KEY=demo-secret-key
PLATON_SUCCESS_REDIRECT=https://example.test/success
PLATON_FAIL_REDIRECT=https://example.test/fail

PLATON_PAYER_EMAIL=payer@example.com
PLATON_CARD_TOKEN=demo-card-token
PLATON_WEBHOOK_URL=https://example.test/webhook

# Mobile wallets (base64 payloads)
PLATON_APPLE_CONTAINER=demo-apple-container
PLATON_GOOGLE_TOKEN=demo-google-token

# Card PAN data
PLATON_CARD_NUMBER=4111111111111111
PLATON_CARD_MONTH=01
PLATON_CARD_YEAR=2026
PLATON_CARD_CVV=123

# Webhook XML payload used by examples/webhook/vebhook.go
PLATON_VALIDATION_SUCCESS=<payments></payments>
